{
  "crons": [],
  "installCommand": "bun install",
  "buildCommand": "bun run build",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate, no-cache, no-store"
        },
        {
          "key": "X-Build-Time",
          "value": "2025-11-25T09:30:00.000Z"
        }
      ]
    }
  ]
}