{
  "version": 2,
  "buildCommand": "bun run build",
  "devCommand": "bun run dev",
  "installCommand": "bun install",
  "framework": null,
  "outputDirectory": ".next",
  "regions": ["sin1", "hkg1", "icn1"],
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "edge"
    }
  },
  "env": {
    "FORCE_REBUILD_TIMESTAMP": "2025-11-25T13:30:00.000Z",
    "BUILD_HASH": "v6.0.0-aggregate-partial-closes"
  }
}