{
  "crons": [],
  "installCommand": "bun install",
  "buildCommand": "rm -rf .next && bun run build",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Build-Time",
          "value": "2025-11-25T10:15:00.000Z"
        }
      ]
    }
  ],
  "regions": ["sin1"]
}